{
  "name": "redux-simple-router",
  "description": "Ruthlessly simple bindings to keep react-router and redux in sync",
  "repos": "https://github.com/jlongster/redux-simple-router.git",
  "main": "lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "deep-equal@^1.0.1"
  ],
  "build": "rm package.json && npm install --prefix . redux-simple-router@1.0.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/redux-simple-router\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.0"
    },
    {
      "version": "1.0.1",
      "build": "rm package.json && npm install --prefix . redux-simple-router@1.0.1"
    },
    {
      "version": "1.0.2",
      "build": "rm package.json && npm install --prefix . redux-simple-router@1.0.2"
    }
  ]
}