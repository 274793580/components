{
  "name": "fit-select",
  "description": "选择框",
  "repos": "https://github.com/2betop/placeholder.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "classnames@^2.2.1",
    "jquery@^2.1.4",
    "lodash@^3.10.1",
    "react@^0.14.3",
    "react-dom@^0.14.3",
    "fit-style@^1.0.25"
  ],
  "build": "rm package.json && npm install --prefix . fit-select@1.0.10",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-select\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.10"
    },
    {
      "version": "1.0.11",
      "build": "rm package.json && npm install --prefix . fit-select@1.0.11"
    },
    {
      "version": "1.0.12",
      "build": "rm package.json && npm install --prefix . fit-select@1.0.12"
    }
  ]
}