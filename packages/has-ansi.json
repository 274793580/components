{
  "build": "rm package.json && npm install --prefix . has-ansi@2.0.0",
  "name": "has-ansi",
  "description": "Check if a string has ANSI escape codes",
  "repos": "https://github.com/sindresorhus/has-ansi",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "ansi-regex@^2.0.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/has-ansi\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/has-ansi\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.0.0"
    }
  ]
}