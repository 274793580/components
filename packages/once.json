{
  "build": "rm package.json && npm install --prefix . once@1.3.0",
  "name": "once",
  "description": "Run a function exactly one time",
  "repos": "https://github.com/isaacs/once",
  "main": "once.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/once\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/once\\/(once\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.3.0"
    },
    {
      "version": "1.3.1",
      "build": "rm package.json && npm install --prefix . once@1.3.1"
    },
    {
      "version": "1.3.2",
      "build": "rm package.json && npm install --prefix . once@1.3.2",
      "dependencies": [
        "wrappy@1"
      ]
    },
    {
      "version": "1.3.3",
      "build": "rm package.json && npm install --prefix . once@1.3.3",
      "repos": "https://github.com/isaacs/once.git",
      "dependencies": [
        "wrappy@1"
      ]
    }
  ]
}