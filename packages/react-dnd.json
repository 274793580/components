{
  "name": "react-dnd",
  "description": "Drag and Drop for React",
  "repos": "https://github.com/gaearon/react-dnd.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "disposables@^1.0.1",
    "dnd-core@^1.2.2",
    "invariant@^2.1.0",
    "lodash@^3.10.1"
  ],
  "build": "rm package.json && npm install --prefix . react-dnd@2.0.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-dnd\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.0.0",
      "__hash": "665904988149b0c208005a0eee8d982a81b3911a"
    },
    {
      "build": "rm package.json && npm install --prefix . react-dnd@2.0.1",
      "version": "2.0.1",
      "__hash": "d4178282abda0f407dd15daf8b2d42e9418dcf93"
    },
    {
      "build": "rm package.json && npm install --prefix . react-dnd@2.0.2",
      "version": "2.0.2",
      "__hash": "6fdef700721c0f1abf33b9db8289301931aa36ea"
    },
    {
      "version": "2.1.2",
      "build": "rm package.json && npm install --prefix . react-dnd@2.1.2",
      "dependencies": [
        "react@^0.14.0"
      ],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/react-dnd\\/(?:test|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/react-dnd\\/dist\\/(.*)$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ],
      "paths": {
        "dist": "."
      }
    }
  ]
}