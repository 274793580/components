{
  "build": "rm package.json && npm install --prefix . rc-cascader@0.5.0",
  "name": "rc-cascader",
  "description": "cascade select ui component for react",
  "repos": "https://github.com/react-component/cascader.git",
  "main": "./lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-cascader\\/(?:test|build|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-cascader\\/(assets\\/\\*\\.css)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-cascader\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-cascader\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.5.0"
    },
    {
      "version": "0.5.1",
      "build": "rm package.json && npm install --prefix . rc-cascader@0.5.1"
    },
    {
      "version": "0.6.0",
      "build": "rm package.json && npm install --prefix . rc-cascader@0.6.0"
    }
  ]
}