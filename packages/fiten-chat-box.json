{
  "build": "rm package.json && npm install --prefix . fiten-chat-box@1.0.3",
  "name": "fiten-chat-box",
  "description": "聊天盒子",
  "repos": "https://github.com/2betop/placeholder.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "classnames@^2.2.3",
    "lodash@^4.0.0",
    "react@^0.14.6",
    "react-dom@^0.14.6",
    "react-list@^0.7.11",
    "react-tappable@^0.8.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fiten-chat-box\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fiten-chat-box\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.3"
    },
    {
      "version": "1.0.4",
      "build": "rm package.json && npm install --prefix . fiten-chat-box@1.0.4"
    }
  ]
}