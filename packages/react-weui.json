{
  "build": "rm package.json && npm install --prefix . react-weui@0.0.0",
  "name": "react-weui",
  "description": "weui for react",
  "repos": "https://github.com/weui/react-weui.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-weui\\/(?:test|build|dist\\/cdn)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-weui\\/(.*\\.(?:js|css))$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.0.0"
    },
    {
      "version": "0.1.0",
      "build": "rm package.json && npm install --prefix . react-weui@0.1.0",
      "main": "./lib",
      "dependencies": [
        "classnames@^2.2.0",
        "react@^0.14.2"
      ],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/react-weui\\/(?:test|build|dist\\/cdn)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/react-weui\\/lib\\/(.*)$",
          "release": "lib/$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    },
    {
