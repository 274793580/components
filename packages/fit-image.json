{
  "build": "rm package.json && npm install --prefix . fit-image@1.0.2",
  "name": "fit-image",
  "description": "tb image wrapper",
  "repos": "https://github.com/tb-component/tb-image.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "classnames@^2.2.3",
    "react@^0.14.6",
    "react-dom@^0.14.6"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-image\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-image\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.2",
      "__hash": "775bd6267fb406df34d0fb04d1067d8dfe01f291"
    },
    {
      "version": "1.0.3",
      "build": "rm package.json && npm install --prefix . fit-image@1.0.3"
    }
  ]
}