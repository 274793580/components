{
  "build": "rm package.json && npm install --prefix . combined-stream@1.0.1",
  "name": "combined-stream",
  "description": "A stream that emits multiple other streams one after another.",
  "repos": "https://github.com/felixge/node-combined-stream.git",
  "main": "lib/combined_stream",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "delayed-stream@^1.0.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/combined-stream\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/combined-stream\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.1"
    },
    {
      "version": "1.0.2",
      "build": "rm package.json && npm install --prefix . combined-stream@1.0.2"
    },
    {
      "version": "1.0.3",
      "build": "rm package.json && npm install --prefix . combined-stream@1.0.3"
    },
    {
      "version": "1.0.4",
      "build": "rm package.json && npm install --prefix . combined-stream@1.0.4"
    },
    {
      "version": "1.0.5",
      "build": "rm package.json && npm install --prefix . combined-stream@1.0.5"
    }
  ]
}