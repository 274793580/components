{
  "name": "rc-tooltip",
  "description": "tooltip ui component for react",
  "repos": "https://github.com/react-component/tooltip.git",
  "main": "lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "rc-trigger@1.x"
  ],
  "build": "rm package.json && npm install --prefix . rc-tooltip@3.1.1",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-tooltip\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-tooltip\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.1.1"
    },
    {
      "version": "3.2.0",
      "build": "rm package.json && npm install --prefix . rc-tooltip@3.2.0"
    },
    {
      "version": "3.3.0",
      "build": "rm package.json && npm install --prefix . rc-tooltip@3.3.0"
    }
  ]
}