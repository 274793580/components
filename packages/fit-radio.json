{
  "name": "fit-modal",
  "description": "模态框",
  "repos": "https://github.com/2betop/placeholder.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "fit-style@^1.0.28"
  ],
  "build": "rm package.json && npm install --prefix . fit-modal@1.0.7",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-modal\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.28",
      "build": "rm package.json && npm install --prefix . fit-radio@1.0.28",
      "name": "fit-radio",
      "description": "单选框",
      "dependencies": [
        "classnames@^2.2.1",
        "react@^0.14.3",
        "fit-button@^1.1.48",
        "fit-style@^1.0.42"
      ],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/fit-radio\\/(?:test|build|dist\\/cdn)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/fit-radio\\/dist\\/(.*)$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ],
      "paths": {
        "dist": "."
      }
    },
    {
      "version": "1.0.31",
      "build": "rm package.json && npm install --prefix . fit-radio@1.0.31",
