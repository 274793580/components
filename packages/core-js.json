{
  "build": "rm package.json && npm install --prefix . core-js@2.0.0",
  "name": "core-js",
  "description": "Standard library",
  "repos": "https://github.com/zloirock/core-js.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/core-js\\/(?:test|build|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/core-js\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/core-js\\/(shim\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/core-js\\/modules\\/(.*)$",
      "release": "modules/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.0.0"
    },
    {
      "version": "2.0.1",
      "build": "rm package.json && npm install --prefix . core-js@2.0.1"
    },
    {
      "version": "2.0.2",
      "build": "rm package.json && npm install --prefix . core-js@2.0.2"
    }
  ]
}