{
  "build": "rm package.json && npm install --prefix . deep-assign@2.0.0",
  "name": "deep-assign",
  "description": "Recursive Object.assign()",
  "repos": "https://github.com/sindresorhus/deep-assign",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "is-obj@^1.0.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/deep-assign\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/deep-assign\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.0.0"
    }
  ]
}