{
  "name": "rc-checkbox",
  "description": "checkbox ui component for react",
  "repos": "https://github.com/react-component/checkbox.git",
  "main": "lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "classnames@2.x"
  ],
  "build": "rm package.json && npm install --prefix . rc-checkbox@1.1.1",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-checkbox\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-checkbox\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.1.1",
      "__hash": "dfc69898ce8c8851b5ad8700429aa09d0b4350d7"
    },
    {
      "version": "1.1.2",
      "build": "rm package.json && npm install --prefix . rc-checkbox@1.1.2",
      "__hash": "9829d3f090ec8733e646075210401911f4a2b2c5"
    }
  ]
}