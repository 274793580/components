    {
      "reg": "^\\/node_modules\\/dnd-core\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
