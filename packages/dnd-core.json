        "redux@^3.2.0"
      ],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/dnd-core\\/(?:test|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/dnd-core\\/lib\\/(.*)$",
          "release": "lib/$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
