  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "invariant@^2.0.0",
    "lodash@^3.10.1",
    "redux@^3.0.0"
  ],
  "build": "rm package.json && npm install --prefix . dnd-core@1.2.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/dnd-core\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
