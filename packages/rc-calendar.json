{
  "repos": "http://github.com/react-component/calendar.git",
  "description": "React Calendar",
  "tag": "master",
  "name": "rc-calendar",
  "main": "lib/index.js",
  "build": "rm package.json && npm install --prefix . rc-calendar@4.0.1",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc\\-calendar\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc\\-calendar\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "4.0.1",
      "__hash": "af6abfc7b97d1fe11d815bbc88e665aa11d88231"
    }
  ]
}