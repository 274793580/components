{
  "repos": "https://github.com/eternicode/bootstrap-datepicker.git",
  "description": "Bootstrap datapicker",
  "main": "bootstrap-datepicker.js",
  "dependencies": [
    "bootstrap@>=2.0.4",
    "jquery@>=1.7.1"
  ],
  "mapping": [
    {
      "reg": "\\.min\\.(js|css)$",
      "release": false
    },
    {
      "reg": "^\\/dist\\/(?:css|js)\\/(.*?)$",
      "release": "$1"
    },
    {
      "reg": "^\\/dist\\/locales\\/(.*?)$",
      "release": "locales/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "shim": {
    "bootstrap-datepicker.js": {
      "replace": {
        "from": {},
        "to": "require('jquery')"
      }
    }
  },
  "tags": [
    {
      "version": "v1.4.0",
      "__hash": "de85f98aa05c4cd93a35580624e4b6ac205d229c"
    }
  ]
}