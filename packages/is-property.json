{
  "build": "rm package.json && npm install --prefix . is-property@1.0.0",
  "name": "is-property",
  "description": "Tests if a JSON property can be accessed using . syntax",
  "repos": "https://github.com/mikolalysenko/is-property.git",
  "main": "is-property.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/is-property\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/is-property\\/(is-property\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.0"
    },
    {
      "version": "1.0.1",
      "build": "rm package.json && npm install --prefix . is-property@1.0.1"
    },
    {
      "version": "1.0.2",
      "build": "rm package.json && npm install --prefix . is-property@1.0.2"
    }
  ]
}