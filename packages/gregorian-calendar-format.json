{
  "name": "gregorian-calendar-format",
  "description": "format utils for gregorian-calendar",
  "repos": "https://github.com/yiminghe/gregorian-calendar-format.git",
  "main": "./lib/gregorian-calendar-format",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "gregorian-calendar@4.x",
    "warning@2.x"
  ],
  "build": "rm package.json && npm install --prefix . gregorian-calendar-format@4.0.4",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/gregorian-calendar-format\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "4.0.4"
    }
  ]
}