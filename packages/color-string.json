{
  "build": "rm package.json && npm install --prefix . color-string@0.3.0",
  "name": "color-string",
  "description": "Parser and generator for CSS color strings",
  "repos": "https://github.com/harthur/color-string.git",
  "main": "color-string",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "color-name@^1.0.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/color-string\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/color-string\\/(color-string\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.3.0"
    }
  ]
}