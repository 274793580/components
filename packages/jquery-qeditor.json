{
  "repos": "https://github.com/huacnlee/jquery.qeditor",
  "description": "This is a simple WYSIWYG editor with jQuery.",
  "name": "jquery-qeditor",
  "main": "jquery.qeditor.js",
  "dependencies": [
    "jquery@>=1.6"
  ],
  "mapping": [
    {
      "reg": "^\\/dist\\/js\\/(jquery\\.qeditor\\.js)",
      "release": "$1"
    },
    {
      "reg": "^\\/dist\\/css\\/(jquery\\.qeditor\\.css)",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "shim": {
    "jquery.qeditor.js": {
      "deps": [
        "jquery"
      ],
      "vars": [
        "jQuery"
      ]
    }
  },
  "tags": [
    {
      "version": "0.1.1"
    }
  ]
}