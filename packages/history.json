{
  "name": "history",
  "description": "A minimal, functional history implementation for JavaScript",
  "repos": "https://github.com/rackt/history.git",
  "main": "lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "deep-equal@^1.0.0",
    "invariant@^2.0.0",
    "query-string@^3.0.0",
    "warning@^2.0.0"
  ],
  "build": "rm package.json && npm install --prefix . history@1.15.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/history\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.15.0"
    },
    {
      "version": "1.16.0",
      "build": "rm package.json && npm install --prefix . history@1.16.0"
    },
    {
      "version": "1.17.0",
      "build": "rm package.json && npm install --prefix . history@1.17.0"
    }
  ]
}
