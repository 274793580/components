{
  "build": "rm package.json && npm install --prefix . animate.css@3.4.0",
  "name": "animate.css",
  "description": "*Just-add-water CSS animation*",
  "repos": "https://github.com/daneden/animate.css.git",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/animate\\.css\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/animate\\.css\\/(animate\\.css)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "main": "animate.css",
  "tags": [
    {
      "version": "3.4.0"
    }
  ]
}