{
  "build": "rm package.json && npm install --prefix . is-regexp@1.0.0",
  "name": "is-regexp",
  "description": "Check whether a variable is a regular expression",
  "repos": "https://github.com/sindresorhus/is-regexp",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/is-regexp\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/is-regexp\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.0",
      "__hash": "6a1b392536149f4dd4cae6e646bf1cb0d7ec4a61"
    }
  ]
