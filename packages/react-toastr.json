{
  "build": "rm package.json && npm install --prefix . react-toastr@2.4.0",
  "name": "react-toastr",
  "description": "React.js toastr component",
  "repos": "https://github.com/tomchentw/react-toastr.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "classnames@^2.2.3",
    "element-class@^0.2.2",
    "react-addons-update@^0.14.6",
    "react-dom@^0.14.6",
    "react@^0.14.6"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-toastr\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-toastr\\/(AUTHORS)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-toastr\\/(CHANGELOG\\.md)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-toastr\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/react-toastr\\/src\\/(.*)$",
      "release": "src/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.4.0"
    }
  ]
}