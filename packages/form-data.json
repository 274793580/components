{
  "build": "rm package.json && npm install --prefix . form-data@1.0.0-rc1",
  "name": "form-data",
  "description": "A module to create readable \"multipart/form-data\" streams.  Can be used to submit forms and file uploads to other web applications.",
  "repos": "https://github.com/felixge/node-form-data.git",
  "main": "lib/form_data",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "async@^1.2.1",
    "combined-stream@^1.0.3",
    "mime-types@^2.1.1"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/form-data\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/form-data\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.0-rc1",
      "__hash": "9efbd43dbaf27a78ddb8c6acaa86f7206a7753c7"
    },
    {
      "version": "1.0.0-rc2",
      "build": "rm package.json && npm install --prefix . form-data@1.0.0-rc2"
    },
    {
      "version": "1.0.0-rc3",
      "build": "rm package.json && npm install --prefix . form-data@1.0.0-rc3",
      "main": "./lib/browser",
      "dependencies": [],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/form-data\\/(?:test|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/form-data\\/(lib\\/browser\\.js)$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    }
  ]
}