{
  "build": "rm package.json && npm install --prefix . component-bind@1.0.0",
  "name": "component-bind",
  "description": "function binding utility",
  "repos": "https://github.com/component/bind.git",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/component-bind\\/(?:test|build|dist\\/cdn)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/component-bind\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "main": "index.js",
  "tags": [
    {
      "version": "1.0.0"
    }
  ]
}