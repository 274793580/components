{
  "build": "rm package.json && npm install --prefix . har-validator@1.6.1",
  "name": "har-validator",
  "description": "Extremely fast HTTP Archive (HAR) validator using JSON Schema",
  "repos": "https://github.com/ahmadnassri/har-validator",
  "main": "./src/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "is-my-json-valid@^2.10.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/har-validator\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/har-validator\\/bin\\/(.*)$",
      "release": "bin/$1"
    },
    {
      "reg": "^\\/node_modules\\/har-validator\\/src\\/(.*)$",
      "release": "src/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
