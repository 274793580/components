{
  "build": "rm package.json && npm install --prefix . react-cropper@0.5.2",
  "name": "react-cropper",
  "description": "Cropper as React components",
  "repos": "https://github.com/roadmanfong/react-cropper.git",
  "main": "react-cropper.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "cropper@^2.2.5",
    "react@^0.14.0",
    "jquery@^1.11.3 || ^2.1.4"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-cropper\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-cropper\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "paths": {
    "dist": "."
  },
  "tags": [
    {
      "version": "0.5.2"
    },
    {
      "version": "0.6.0",
      "build": "rm package.json && npm install --prefix . react-cropper@0.6.0",
      "dependencies": [
        "react@^0.14.0"
      ]
    }
  ]
}