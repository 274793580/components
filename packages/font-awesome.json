{
  "repos": "https://github.com/FortAwesome/Font-Awesome.git",
  "name": "font-awesome",
  "main": "css/font-awesome.css",
  "description": "The iconic font and CSS toolkit http://fontawesome.io",
  "mapping": [
    {
      "reg": "\\.min\\.(js|css)$",
      "release": false
    },
    {
      "reg": "^\\/css\\/(.*?)$",
      "release": "$&"
    },
    {
      "reg": "^\\/scss\\/(.*?)$",
      "release": "$&"
    },
    {
      "reg": "^\\/less\\/(.*?)$",
      "release": "$&"
    },
    {
      "reg": "^\\/fonts\\/(.*?)$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "v4.2.0",
      "__hash": "75ca5658d8a2a2e25a99f9d4d9ff85ea042a15b4"
    },
    {
      "version": "4.5.0",
      "build": "rm package.json && npm install --prefix . font-awesome@4.5.0",
      "description": "The iconic font and CSS framework",
      "tag": "master",
      "reposType": "npm",
      "dependencies": [],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/font-awesome\\/(?:test|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/font-awesome\\/fonts\\/(.*)$",
          "release": "fonts/$1"
        },
        {
          "reg": "^\\/node_modules\\/font-awesome\\/css\\/(.*)$",
          "release": "css/$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ],
      "main": "index.js",
      "__hash": "295b8972602bdb8e4c4bb62d179516dd70bb660c"
    }
  ]
}