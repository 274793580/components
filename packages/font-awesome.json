{
  "repos": "https://github.com/FortAwesome/Font-Awesome.git",
  "name": "font-awesome",
  "main": "css/font-awesome.css",
  "description": "The iconic font and CSS toolkit http://fontawesome.io",
  "mapping": [
    {
      "reg": "\\.min\\.(js|css)$",
      "release": false
    },
    {
      "reg": "^\\/css\\/(.*?)$",
      "release": "$&"
    },
    {
      "reg": "^\\/scss\\/(.*?)$",
      "release": "$&"
    },
    {
      "reg": "^\\/less\\/(.*?)$",
      "release": "$&"
    },
    {
      "reg": "^\\/fonts\\/(.*?)$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "v4.2.0",
      "__hash": "75ca5658d8a2a2e25a99f9d4d9ff85ea042a15b4"
    }
  ]
}