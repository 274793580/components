{
  "build": "rm package.json && npm install --prefix . nock@6.0.1",
  "name": "nock",
  "description": "HTTP Server mocking for Node.js",
  "repos": "https://github.com/pgte/nock.git",
  "main": "index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "chai@>=1.9.2 <4.0.0",
    "debug@^2.2.0",
    "deep-equal@^1.0.0",
    "json-stringify-safe@^5.0.1",
    "lodash@^3.10.1",
    "mkdirp@^0.5.0",
    "propagate@0.3.x",
    "qs@^6.0.2"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/nock\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/nock\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/nock\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/nock\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "6.0.1",
      "__hash": "47d352f54973bb1fffb4296baf5c0f3eb4d37ab8"
    },
    {
      "version": "7.0.0",
      "build": "rm package.json && npm install --prefix . nock@7.0.0"
    },
    {
      "version": "7.0.1",
      "build": "rm package.json && npm install --prefix . nock@7.0.1"
    }
  ]
}