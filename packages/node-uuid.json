{
  "build": "rm package.json && npm install --prefix . node-uuid@1.4.0",
  "name": "node-uuid",
  "description": "Rigorous implementation of RFC4122 (v1 and v4) UUIDs.",
  "repos": "https://github.com/broofa/node-uuid.git",
  "main": "uuid.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/node-uuid\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/node-uuid\\/(uuid\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.4.0"
    },
    {
      "version": "1.4.1",
      "build": "rm package.json && npm install --prefix . node-uuid@1.4.1"
    },
    {
      "version": "1.4.2",
      "build": "rm package.json && npm install --prefix . node-uuid@1.4.2"
    },
    {
      "version": "1.4.3",
      "build": "rm package.json && npm install --prefix . node-uuid@1.4.3"
    },
    {
      "version": "1.4.6",
      "build": "rm package.json && npm install --prefix . node-uuid@1.4.6"
    },
    {
      "version": "1.4.7",
      "build": "rm package.json && npm install --prefix . node-uuid@1.4.7"
    }
  ]
}