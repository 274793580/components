{
  "name": "fit-message",
  "description": "全局提示",
  "repos": "https://github.com/2betop/placeholder.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "classnames@^2.2.1"
  ],
  "build": "rm package.json && npm install --prefix . fit-message@1.0.2",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-message\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.0",
      "build": "rm package.json && npm install --prefix . fit-mobile@1.0.0",
      "name": "fit-mobile",
      "description": "fit mobile",
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/fit-mobile\\/(?:test|build|dist\\/cdn)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/fit-mobile\\/(.*\\.(?:js|css))$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    },
    {
      "version": "1.0.1",
      "build": "rm package.json && npm install --prefix . fit-mobile@1.0.1",
      "name": "fit-mobile",
      "description": "fit mobile",
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/fit-mobile\\/(?:test|build|dist\\/cdn)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/fit-mobile\\/(.*\\.(?:js|css))$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    }
  ]
}