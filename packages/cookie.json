{
  "build": "rm package.json && npm install --prefix . cookie@0.2.3",
  "name": "cookie",
  "description": "cookie parsing and serialization",
  "repos": "https://github.com/jshttp/cookie",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/cookie\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/cookie\\/(HISTORY\\.md)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/cookie\\/(LICENSE)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/cookie\\/(README\\.md)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/cookie\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.2.3",
      "__hash": "bb62cc575e03bdee11f60d35cf89ae6aa6832b9b"
    }
  ]
}