{
  "build": "rm package.json && npm install --prefix . platform@1.2.0",
  "name": "platform",
  "description": "A platform detection library that works on nearly all JavaScript platforms.",
  "repos": "https://github.com/bestiejs/platform.js",
  "main": "platform.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/platform\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/platform\\/(LICENSE\\.txt)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/platform\\/(platform\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.2.0"
    },
    {
      "version": "1.3.0",
      "build": "rm package.json && npm install --prefix . platform@1.3.0"
    },
    {
      "version": "1.3.1",
      "build": "rm package.json && npm install --prefix . platform@1.3.1"
    }
  ]
}