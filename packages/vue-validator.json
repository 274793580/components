{
  "repos": "https://github.com/vuejs/vue-validator",
  "description": "Validator component for VueJS",
  "name": "vue-validator",
  "main": "vue-validator.js",
  "dependencies": [
    "vue@>=0.11.2"
  ],
  "mapping": [
    {
      "reg": "^\\/dist\\/(vue-validator\\.js)",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "v1.0.7"
    },
    {
      "version": "v1.2.0",
      "dependencies": [
        "vue@>=0.12.0"
      ]
    },
    {
      "version": "v1.3.3",
      "dependencies": [
        "vue@>=0.12.0"
      ]
    },
    {
      "version": "v1.4.1",
      "dependencies": [
        "vue@>=0.12.0"
      ]
    },
    {
      "version": "v1.4.3",
      "dependencies": [
        "vue@>=0.12.0"
      ]
    }
  ]
}