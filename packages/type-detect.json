{
  "build": "rm package.json && npm install --prefix . type-detect@1.0.0",
  "name": "type-detect",
  "description": "Improved typeof detection for node.js and the browser.",
  "repos": "https://github.com/git@github.com:chaijs/type-detect.git",
  "main": "index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/type-detect\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/type-detect\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/type-detect\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.1.1",
      "build": "rm package.json && npm install --prefix . type-detect@0.1.1",
