{
  "build": "rm package.json && npm install --prefix . react-infinite@0.7.3",
  "name": "react-infinite",
  "description": "A browser-ready efficient scrolling container based on UITableView",
  "repos": "https://github.com/seatgeek/react-infinite.git",
  "main": "build/react-infinite.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "lodash.isarray@3.0.4",
    "lodash.isfinite@3.2.0",
    "object-assign@4.0.1",
    "react@^0.14.0",
    "react-dom@^0.14.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(?:test|build|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/react-infinite\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/utils\\/establish-polyfills\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/utils\\/scaleEnum\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/utils\\/infiniteHelpers\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/computers\\/constantInfiniteComputer\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/computers\\/infiniteComputer\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/computers\\/arrayInfiniteComputer\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/utils\\/binaryIndexSearch\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/utils\\/types\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/(build\\/utils\\/checkProps\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/react-infinite\\/dist\\/(.*)$",
      "release": "dist/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.7.3"
    }
  ]
}