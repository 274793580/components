{
  "build": "rm package.json && npm install --prefix . hoek@3.0.0",
  "name": "hoek",
  "description": "General purpose node utilities",
  "repos": "https://github.com/hapijs/hoek.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/hoek\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/hoek\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.0.0"
    },
    {
      "version": "3.0.1",
      "build": "rm package.json && npm install --prefix . hoek@3.0.1"
    },
    {
      "version": "3.0.2",
      "build": "rm package.json && npm install --prefix . hoek@3.0.2"
    },
    {
      "version": "3.0.3",
      "build": "rm package.json && npm install --prefix . hoek@3.0.3"
    },
    {
      "version": "3.0.4",
      "build": "rm package.json && npm install --prefix . hoek@3.0.4"
    }
  ]
}