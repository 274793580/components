{
  "build": "rm package.json && npm install --prefix . react-motion@0.4.0",
  "name": "react-motion",
  "description": "A spring that solves your animation problems.",
  "repos": "https://github.com/chenglou/react-motion.git",
  "main": "lib/react-motion.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "performance-now@^0.2.0",
    "raf@^3.1.0",
    "react@>=0.13.2 || ^0.14"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-motion\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-motion\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.4.0"
    },
    {
      "version": "0.4.1",
      "build": "rm package.json && npm install --prefix . react-motion@0.4.1"
    },
    {
      "version": "0.4.2",
      "build": "rm package.json && npm install --prefix . react-motion@0.4.2"
    }
  ]
}