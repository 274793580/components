{
  "build": "rm package.json && npm install --prefix . thread-sleep@0.0.1",
  "name": "thread-sleep",
  "description": "A native module for when you just need node to back off for a few milliseconds",
  "repos": "https://github.com/ForbesLindesay/thread-sleep.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "node-pre-gyp@^0.6.1"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/thread-sleep\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/thread-sleep\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/thread-sleep\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.0.1"
    },
    {
      "version": "0.0.2",
      "build": "rm package.json && npm install --prefix . thread-sleep@0.0.2"
    },
    {
      "version": "0.0.3",
      "build": "rm package.json && npm install --prefix . thread-sleep@0.0.3"
    },
    {
      "version": "0.0.4",
      "build": "rm package.json && npm install --prefix . thread-sleep@0.0.4",
      "dependencies": [
        "node-pre-gyp@^0.6.2"
      ]
    },
    {
      "version": "0.0.5",
