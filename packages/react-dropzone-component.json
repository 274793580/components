{
  "build": "rm package.json && npm install --prefix . react-dropzone-component@0.7.0",
  "name": "react-dropzone-component",
  "description": "A Dropzone Component for ReactJS",
  "repos": "https://github.com/felixrieseberg/React-Dropzone.git",
  "main": "lib/react-dropzone.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "dropzone@^4.2.0",
    "react-dom@^0.14.0",
    "react@>=0.13 || >0.14.0-alpha"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-dropzone-component\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-dropzone-component\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "paths": {
    "dist": "."
  },
  "tags": [
    {
      "version": "0.7.0"
    },
    {
      "version": "0.8.0",
      "build": "rm package.json && npm install --prefix . react-dropzone-component@0.8.0"
    },
    {
      "version": "0.8.1",
      "build": "rm package.json && npm install --prefix . react-dropzone-component@0.8.1"
    }
  ]
}