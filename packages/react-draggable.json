{
  "build": "rm package.json && npm install --prefix . react-draggable@1.1.1",
  "name": "react-draggable",
  "description": "React draggable component",
  "repos": "https://github.com/mzabriskie/react-draggable.git",
  "main": "dist/react-draggable.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "classnames@^2.2.0",
    "object-assign@^4.0.1"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-draggable\\/(?:test|build|dist\\/cdn)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-draggable\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/react-draggable\\/(dist\\/react-draggable\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.1.1"
    },
    {
      "version": "1.1.2",
      "build": "rm package.json && npm install --prefix . react-draggable@1.1.2"
    },
    {
      "version": "1.1.3",
      "build": "rm package.json && npm install --prefix . react-draggable@1.1.3"
    }
  ]
}