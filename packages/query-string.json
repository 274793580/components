{
  "name": "query-string",
  "description": "Parse and stringify URL query strings",
  "repos": "https://github.com/sindresorhus/query-string",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "strict-uri-encode@^1.0.0"
  ],
  "build": "rm package.json && npm install --prefix . query-string@2.4.1",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/query-string\\/(.*\\.(?:js|css))$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.4.1",
