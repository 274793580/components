{
  "build": "rm package.json && npm install --prefix . isstream@0.1.1",
  "name": "isstream",
  "description": "Determine if an object is a Stream",
  "repos": "https://github.com/rvagg/isstream.git",
  "main": "isstream.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/isstream\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/isstream\\/(isstream\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.1.1"
    },
    {
      "version": "0.1.2",
      "build": "rm package.json && npm install --prefix . isstream@0.1.2"
    }
  ]
}