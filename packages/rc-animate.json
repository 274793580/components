  "main": "./lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "css-animation@1.x"
  ],
  "build": "rm package.json && npm install --prefix . rc-animate@2.0.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
