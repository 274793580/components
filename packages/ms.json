{
  "build": "rm package.json && npm install --prefix . ms@0.7.1",
  "name": "ms",
  "description": "Tiny ms conversion utility",
  "repos": "https://github.com/guille/ms.js.git",
  "main": "index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/ms\\/(?:test|build|dist\\/cdn)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/ms\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.7.1",
      "__hash": "620265249acd79b2d7c07fbf2edafe40a6384bc9"
    }
  ]
}