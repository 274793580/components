{
  "build": "rm package.json && npm install --prefix . d3-sankey@0.1.3",
  "name": "d3-sankey",
  "description": "D3 4.0 implementation of the Sankey plugin",
  "repos": "https://github.com/xaranke/d3-sankey.git",
  "main": "build/d3-sankey.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "d3-arrays@^0.4.0",
    "d3-interpolate@^0.2.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/d3-sankey\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/d3-sankey\\/(build\\/d3-sankey\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.1.3"
    },
    {
      "version": "0.2.0",
      "build": "rm package.json && npm install --prefix . d3-sankey@0.2.0"
    },
    {
      "version": "0.2.1",
      "build": "rm package.json && npm install --prefix . d3-sankey@0.2.1",
      "repos": "https://github.com/d3/d3-sankey.git"
    }
  ]
}