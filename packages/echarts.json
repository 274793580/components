{
  "repos": "https://github.com/ecomfe/echarts.git",
  "name": "echarts",
  "main": "echarts.js",
  "dependencies": [
    "zrender"
  ],
  "umd2commonjs": true,
  "mapping": [
    {
      "reg": "\\.min\\.(js|css)$",
      "release": false
    },
    {
      "reg": "\\/util\\/mapData\\/rawData\\/.*$",
      "release": false
    },
    {
      "reg": "^\\/src\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.1.10",
      "__hash": "afab0bebd6f43ee8601f2e9b7e836767ce77ab03"
    },
    {
      "version": "2.2.7",
      "__hash": "f72b26ff16042e999799c997d491efea252e54b0"
    },
    {
      "version": "3.0.2",
      "__hash": "f2a33ad29cc6aa9383c39082ca6e80dc3ad59fae"
    },
    {
      "version": "3.1.6",
      "build": "rm package.json && npm install --prefix . echarts@3.1.6",
      "description": "A powerful charting and visualization library for browser",
      "tag": "master",
      "reposType": "npm",
      "dependencies": [
        "zrender@^3.0.7"
      ],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/echarts\\/(?:test|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/echarts\\/(index\\.js)$",
          "release": "$1"
        },
        {
          "reg": "^\\/node_modules\\/echarts\\/lib\\/(.*)$",
          "release": "lib/$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ],
      "main": "index",
      "umd2commonjs": null,
      "__hash": "e7d4750c69eb8b9ebda8552917e5957952565b7e"
    }
  ]
}