{
  "name": "dom-scroll-into-view",
  "description": "scroll dom node into view automatically",
  "repos": "https://github.com/git@github.com:yiminghe/dom-scroll-into-view.git",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . dom-scroll-into-view@1.0.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/dom-scroll-into-view\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/dom-scroll-into-view\\/(.*\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/dom-scroll-into-view\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.0",
      "__hash": "e8460fbf3d063ce77c217c19de5f230fbed873d8"
    },
    {
      "version": "1.0.1",
      "build": "rm package.json && npm install --prefix . dom-scroll-into-view@1.0.1",
      "__hash": "a8fb65afc173d0c6da30a624be1b2011a9464368"
    }
  ]
}