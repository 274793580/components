{
  "build": "rm package.json && npm install --prefix . react-lazy-load@2.0.0",
  "name": "react-lazy-load",
  "description": "Simple lazy loading component built with react",
  "repos": "https://github.com/loktar00/react-lazy-load.git",
  "main": "lib/LazyLoad.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "react@^0.14.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-lazy-load\\/(?:test|build|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-lazy-load\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "paths": {
    "dist": "."
  },
  "tags": [
    {
      "version": "2.0.0"
    },
    {
      "version": "2.0.1",
      "build": "rm package.json && npm install --prefix . react-lazy-load@2.0.1"
    },
    {
      "version": "2.0.2",
      "build": "rm package.json && npm install --prefix . react-lazy-load@2.0.2"
    }
  ]
}