{
  "name": "rc-dropdown",
  "description": "dropdown ui component for react",
  "repos": "https://github.com/react-component/dropdown.git",
  "main": "lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "rc-trigger@1.x"
  ],
  "build": "rm package.json && npm install --prefix . rc-dropdown@1.4.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-dropdown\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-dropdown\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.4.0",
      "__hash": "de571978836a4e0111cd7976f27b07152ddfde15"
    },
    {
      "build": "rm package.json && npm install --prefix . rc-dropdown@1.4.2",
      "version": "1.4.2",
      "__hash": "dcfaec3595df89d35b0e340941f66011c3703e78"
    },
    {
      "version": "1.4.3",
      "build": "rm package.json && npm install --prefix . rc-dropdown@1.4.3",
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/rc-dropdown\\/(?:test|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/rc-dropdown\\/(assets\\/\\*\\.css)$",
          "release": "$1"
        },
        {
          "reg": "^\\/node_modules\\/rc-dropdown\\/lib\\/(.*)$",
          "release": "lib/$1"
        },
        {
          "reg": "^\\/node_modules\\/rc-dropdown\\/assets\\/(.*)$",
          "release": "assets/$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    }
  ]
}