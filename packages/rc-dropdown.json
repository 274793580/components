{
  "name": "rc-dropdown",
  "description": "dropdown ui component for react",
  "repos": "https://github.com/react-component/dropdown.git",
  "main": "lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "rc-trigger@1.x"
  ],
  "build": "rm package.json && npm install --prefix . rc-dropdown@1.4.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-dropdown\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-dropdown\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.4.0"
    },
    {
      "version": "1.4.2",
      "build": "rm package.json && npm install --prefix . rc-dropdown@1.4.2"
    },
    {
      "version": "1.4.3",
      "build": "rm package.json && npm install --prefix . rc-dropdown@1.4.3"
    }
  ]
}