{
  "build": "rm package.json && npm install --prefix . os-tmpdir@1.0.0",
  "name": "os-tmpdir",
  "description": "Node.js os.tmpdir() ponyfill",
  "repos": "https://github.com/sindresorhus/os-tmpdir.git",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/os-tmpdir\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/os-tmpdir\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.0"
    },
    {
      "version": "1.0.1",
      "build": "rm package.json && npm install --prefix . os-tmpdir@1.0.1"
    }
  ]
}