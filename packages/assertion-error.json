{
  "build": "rm package.json && npm install --prefix . assertion-error@1.0.0",
  "name": "assertion-error",
  "description": "Error constructor for test and validation frameworks that implements standardized AssertionError specification.",
  "repos": "https://github.com/git@github.com:chaijs/assertion-error.git",
  "main": "index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/assertion-error\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/assertion-error\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.0",
      "__hash": "76b6dce1e53fb7ca215a656fc00d26bac46071c6"
    }
  ]
}