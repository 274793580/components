{
  "repos": "https://github.com/requirejs/text.git",
  "name": "text",
  "main": "text.js",
  "mapping": [
    {
      "reg": "\\.min\\.(js|css)$",
      "release": false
    },
    {
      "reg": "^\\/text\\.js$",
      "release": "$&"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.0.12"
    }
  ]
}