{
  "repos": "https://github.com/lodash/lodash.git",
  "tag": "master",
  "name": "lodash.throttle",
  "main": "index.js",
  "build": "rm package.json && npm install --prefix . lodash.throttle@3.0.4",
  "dependencies": [
    "lodash.debounce@^3.0.0"
  ],
  "mapping": [
    {
      "reg": "^\\/node_modules\\/lodash\\.throttle\\/index\\.js$",
      "release": "index.js"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.0.4",
      "description": "The modern build of lodashâ€™s `_.throttle` as a module.",
      "reposType": "npm",
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/lodash\\.throttle\\/(?:test|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/lodash\\.throttle\\/(index\\.js)$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    }
  ]
}