{
  "build": "rm package.json && npm install --prefix . redux@3.2.1",
  "name": "redux",
  "description": "Predictable state container for JavaScript apps",
  "repos": "https://github.com/rackt/redux.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "lodash@^4.2.1"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/redux\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/redux\\/(es)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/redux\\/dist\\/(.*)$",
      "release": "dist/$1"
    },
    {
      "reg": "^\\/node_modules\\/redux\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/redux\\/src\\/(.*)$",
      "release": "src/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
