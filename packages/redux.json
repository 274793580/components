{
  "repos": "https://github.com/rackt/redux.git",
  "name": "redux",
  "main": "redux.js",
  "build": "rm package.json && npm install --prefix . redux@3.0.4",
  "mapping": [
    {
      "reg": "\\bmin\\b",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/redux\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "v3.0.4",
      "__hash": "dfe0dc623be1a1e9132e0490b6cecfd65f68ff09"
    },
    {
      "description": "Predictable state container for JavaScript apps",
      "tag": "master",
      "reposType": "npm",
      "version": "3.0.5",
      "build": "rm package.json && npm install --prefix . redux@3.0.5",
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/redux\\/lib\\/(.*)$",
          "release": "lib/$1"
        },
        {
          "reg": "^\\/node_modules\\/redux\\/dist\\/(.*)$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    }
  ]
}