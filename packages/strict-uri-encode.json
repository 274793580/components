{
  "name": "strict-uri-encode",
  "description": "A stricter URI encode adhering to RFC 3986",
  "repos": "https://github.com/kevva/strict-uri-encode.git",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . strict-uri-encode@1.0.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/strict-uri-encode\\/(.*\\.(?:js|css))$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
