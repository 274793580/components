{
  "name": "strict-uri-encode",
  "description": "A stricter URI encode adhering to RFC 3986",
  "repos": "https://github.com/kevva/strict-uri-encode.git",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . strict-uri-encode@1.0.0",
  "mapping": [
    {
      "reg": "/\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js/i",
      "release": false
    },
    {
      "reg": "/^\\/node_modules\\/strict-uri-encode\\/(.*\\.(?:js|css))$/i",
      "release": "$1"
    },
    {
      "reg": "/^\\/README\\.md$/i",
      "release": "$&"
    },
    {
      "reg": "/^\\/LICENSE\\.md$/i",
      "release": "$&"
    },
    {
      "reg": "*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.0"
    },
    {
      "version": "1.0.1",
      "build": "rm package.json && npm install --prefix . strict-uri-encode@1.0.1"
    },
    {
      "version": "1.0.2",
      "build": "rm package.json && npm install --prefix . strict-uri-encode@1.0.2"
    }
  ]
}