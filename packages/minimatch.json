{
  "build": "rm package.json && npm install --prefix . minimatch@0.2.0",
  "name": "minimatch",
  "description": "a glob matcher in javascript",
  "repos": "https://github.com/isaacs/minimatch.git",
  "main": "minimatch.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "lru-cache@~1.0.5"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/minimatch\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/minimatch\\/(minimatch\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
