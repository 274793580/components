{
  "build": "rm package.json && npm install --prefix . scroll-behavior@0.3.3",
  "name": "scroll-behavior",
  "description": "Scroll behaviors for use with history",
  "repos": "https://github.com/taion/scroll-behavior.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "dom-helpers@^2.4.0",
    "history@^1.12.1 || ^2.0.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/scroll-behavior\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/scroll-behavior\\/es6\\/(.*)$",
      "release": "es6/$1"
    },
    {
      "reg": "^\\/node_modules\\/scroll-behavior\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.3.3"
    },
    {
      "version": "0.3.4",
      "build": "rm package.json && npm install --prefix . scroll-behavior@0.3.4"
    },
    {
      "version": "0.4.0",
      "build": "rm package.json && npm install --prefix . scroll-behavior@0.4.0",
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/scroll-behavior\\/(?:test|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/scroll-behavior\\/(es6)$",
          "release": "$1"
        },
        {
          "reg": "^\\/node_modules\\/scroll-behavior\\/lib\\/(.*)$",
          "release": "lib/$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    }
  ]
}