{
  "name": "can-use-dom",
  "description": "Test if you can use dom in the current environment",
  "repos": "https://github.com/akiran/can-use-dom.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . can-use-dom@0.1.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/can-use-dom\\/(.*\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.1.0",
      "__hash": "b62a0f86874e6dcbed5e556127daa4b0679e4147"
    }
  ]
}