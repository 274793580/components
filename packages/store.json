{
  "build": "rm package.json && npm install --prefix . store@1.3.17",
  "name": "store",
  "description": "A localStorage wrapper for all browsers without using cookies or flash. Uses localStorage, globalStorage, and userData behavior under the hood",
  "repos": "https://github.com/marcuswestin/store.js.git",
  "main": "store",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/store\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/store\\/(store\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.3.17"
    },
    {
      "version": "1.3.19",
      "build": "rm package.json && npm install --prefix . store@1.3.19"
    },
    {
      "version": "1.3.20",
      "build": "rm package.json && npm install --prefix . store@1.3.20",
      "main": "store.js"
    }
  ]
}