{
  "build": "rm package.json && npm install --prefix . react-mixin@3.0.3",
  "name": "react-mixin",
  "description": "![travis](https://travis-ci.org/brigand/react-mixin.svg)",
  "repos": "https://github.com/brigand/react-mixin.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "object-assign@^4.0.1",
    "smart-mixin@^1.2.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-mixin\\/(?:test|build|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-mixin\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.0.3"
    }
  ]
}