{
  "build": "rm package.json && npm install --prefix . color@0.11.1",
  "name": "color",
  "description": "Color conversion and manipulation with CSS string support",
  "repos": "https://github.com/MoOx/color.git",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "color-convert@^0.5.3",
    "color-string@^0.3.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/color\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/color\\/(CHANGELOG\\.md)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/color\\/(LICENSE)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/color\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.11.1"
    }
  ]
}