{
  "build": "rm package.json && npm install --prefix . ansi-styles@2.1.0",
  "name": "ansi-styles",
  "description": "ANSI escape codes for styling strings in the terminal",
  "repos": "https://github.com/chalk/ansi-styles",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/ansi-styles\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/ansi-styles\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.1.0",
      "__hash": "72367333cc08b7465ec0911fd083e3e04d6306b7"
    }
  ]
}