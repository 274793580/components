{
  "repos": "https://github.com/angular/bower-angular-route",
  "description": "HTML enhanced for web apps http://angularjs.org",
  "name": "angular-route",
  "main": "angular-route.js",
  "mapping": [
    {
      "reg": "^\\/angular-route\\.js$",
      "release": "$&"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "shim": {
    "angular-route.js": {
      "deps": [
        "angular"
      ]
    }
  },
  "dependencies": [
    "angular@~1.4.0"
  ],
  "tags": [
    {
      "version": "v1.4.0"
    },
    {
      "version": "v1.3.14",
      "dependencies": [
        "angular@~1.3.14"
      ]
    }
  ]
}