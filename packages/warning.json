{
  "repos": "https://github.com/r3dm/warning.git",
  "name": "warning",
  "main": "warning.js",
  "mapping": [
    {
      "reg": "\\bmin\\b",
      "release": false
    },
    {
      "reg": "^\\/(browser\\.js)$",
      "release": "warning.js"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "shim": {
    "warning.js": {
      "replace": {
        "from": "process.env.NODE_ENV !== 'production'",
        "to": "true"
      }
    }
  },
  "tags": [
    {
      "version": "v2.1.0"
    }
  ]
}