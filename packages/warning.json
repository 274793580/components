{
  "name": "warning",
  "description": "A mirror of Facebook's Warning",
  "repos": "https://github.com/r3dm/warning.git",
  "main": "browser.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "build": "rm package.json && npm install --prefix . warning@1.0.2",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/warning\\/(browser\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.2"
    },
    {
      "version": "2.0.0",
      "build": "rm package.json && npm install --prefix . warning@2.0.0"
    },
    {
      "version": "2.1.0",
      "build": "rm package.json && npm install --prefix . warning@2.1.0"
    }
  ]
}