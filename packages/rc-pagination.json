{
  "name": "rc-pagination",
  "description": "pagination ui component for react",
  "repos": "https://github.com/react-component/pagination.git",
  "main": "lib/index",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . rc-pagination@1.3.4-beta",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-pagination\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-pagination\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.3.4-beta"
    },
    {
      "version": "1.3.4",
      "build": "rm package.json && npm install --prefix . rc-pagination@1.3.4"
    },
    {
      "version": "1.3.5",
      "build": "rm package.json && npm install --prefix . rc-pagination@1.3.5"
    }
  ]
}