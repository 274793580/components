{
  "name": "fit-layout",
  "description": "布局组件",
  "repos": "https://github.com/2betop/placeholder.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "classnames@^2.2.1",
    "fit-style@^1.0.28"
  ],
  "build": "rm package.json && npm install --prefix . fit-layout@1.0.19",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-layout\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.19"
    },
    {
      "version": "1.0.21",
      "build": "rm package.json && npm install --prefix . fit-layout@1.0.21"
    },
    {
      "version": "1.0.22",
      "build": "rm package.json && npm install --prefix . fit-layout@1.0.22"
    }
  ]
}