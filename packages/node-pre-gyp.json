{
  "build": "rm package.json && npm install --prefix . node-pre-gyp@0.6.1",
  "name": "node-pre-gyp",
  "description": "Node.js native addon binary install tool",
  "repos": "https://github.com/mapbox/node-pre-gyp.git",
  "main": "lib/node-pre-gyp.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "nopt@~3.0.1",
    "npmlog@~0.1.1",
    "request@2.x",
    "semver@~4.1.0",
    "tar@~1.0.2",
    "tar-pack@~2.0.0",
    "mkdirp@~0.5.0",
    "rc@~0.5.1",
    "rimraf@~2.2.8"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/node-pre-gyp\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/node-pre-gyp\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
