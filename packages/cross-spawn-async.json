{
  "build": "rm package.json && npm install --prefix . cross-spawn-async@2.0.0",
  "name": "cross-spawn-async",
  "description": "Cross platform child_process#spawn",
  "repos": "https://github.com/IndigoUnited/node-cross-spawn-async.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "lru-cache@^2.6.5",
    "which@^1.1.1"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/cross-spawn-async\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/cross-spawn-async\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/cross-spawn-async\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
