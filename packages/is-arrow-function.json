{
  "build": "rm package.json && npm install --prefix . is-arrow-function@2.0.3",
  "name": "is-arrow-function",
  "description": "Determine if a function is an ES6 arrow function or not.",
  "repos": "https://github.com/ljharb/is-arrow-function.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "is-callable@^1.0.4"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/is-arrow-function\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/is-arrow-function\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.0.3",
      "__hash": "0312934c8e21fb3e9999ba876842722d0a1e1a81"
    }
  ]
}