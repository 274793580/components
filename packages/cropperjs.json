{
  "build": "rm package.json && npm install --prefix . cropperjs@0.5.0",
  "name": "cropperjs",
  "description": "JavaScript image cropper.",
  "repos": "https://github.com/fengyuanchen/cropperjs.git",
  "main": "cropper.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/cropperjs\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/cropperjs\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/cropperjs\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "paths": {
    "dist": "."
