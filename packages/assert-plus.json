{
  "build": "rm package.json && npm install --prefix . assert-plus@0.1.5",
  "name": "assert-plus",
  "description": "Extra assertions on top of node's assert module",
  "repos": "https://github.com/mcavage/node-assert-plus.git",
  "main": "assert.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/assert-plus\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/assert-plus\\/(assert\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.1.5",
      "__hash": "87cd872947439bff792f764f48361cb7334f8cb8"
    }
  ]
}