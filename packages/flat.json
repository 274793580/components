{
  "build": "rm package.json && npm install --prefix . flat@1.6.0",
  "name": "flat",
  "description": "Take a nested Javascript object and flatten it, or unflatten an object with delimited keys",
  "repos": "https://github.com/hughsk/flat.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/flat\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/flat\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.6.0",
      "__hash": "364ae8a76febe76968d0da046f8bea901b21ead1"
    },
    {
      "version": "1.6.1",
      "build": "rm package.json && npm install --prefix . flat@1.6.1"
    }
  ]
}