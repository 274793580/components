{
  "build": "rm package.json && npm install --prefix . superagent@1.6.1",
  "name": "superagent",
  "description": "elegant & feature rich browser / node HTTP with a fluent API",
  "repos": "https://github.com/visionmedia/superagent.git",
  "main": "./lib/client.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/superagent\\/(?:test|build|dist\\/cdn)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/superagent\\/(lib\\/client\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "paths": {
    "./lib/node/index.js": "./lib/client.js",
    "./test/support/server.js": "./test/support/blank.js"
  },
  "shim": {
    "**/*.js": {
      "replace": [
        {
          "from": "/\\brequire\\s*\\(\\s*('|\")emitter\\b/ig",
          "to": "require($1component-emitter"
        },
        {
          "from": "/\\brequire\\s*\\(\\s*('|\")reduce\\b/ig",
          "to": "require($1reduce-component"
        }
      ]
    }
  },
  "tags": [
    {
      "version": "1.6.1",
      "__hash": "07c12630e6b8aae6916ca98a9c12f7f416dca184"
    }
  ]
}