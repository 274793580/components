{
  "build": "rm package.json && npm install --prefix . fit-timeago@0.2.25",
  "name": "fit-timeago",
  "description": "time-ago",
  "repos": "https://github.com/tb-component/pc-timeago.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-timeago\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-timeago\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.2.25"
    },
    {
      "version": "0.2.26",
      "build": "rm package.json && npm install --prefix . fit-timeago@0.2.26"
    }
  ]
}