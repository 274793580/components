{
  "build": "rm package.json && npm install --prefix . d3-interpolate@0.2.0",
  "name": "d3-interpolate",
  "description": "Interpolate numbers, colors, strings, arrays, objects, whatever!",
  "repos": "https://github.com/d3/d3-interpolate.git",
  "main": "build/d3-interpolate.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "d3-color@~0.3.1"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/d3-interpolate\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/d3-interpolate\\/(build\\/d3-interpolate\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.2.0",
      "__hash": "4196dc9f211d93334370a772a521c9db5f9bc774"
    },
    {
      "version": "0.2.1",
      "build": "rm package.json && npm install --prefix . d3-interpolate@0.2.1"
    }
  ]
}