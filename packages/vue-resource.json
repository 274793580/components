{
  "repos": "https://github.com/vuejs/vue-resource",
  "description": "Resource component for Vue.js",
  "name": "vue-resource",
  "main": "vue-resource.js",
  "dependencies": [
    "vue@>=0.12.0"
  ],
  "mapping": [
    {
      "reg": "^\\/dist\\/(vue-resource\\.js)",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.1.13",
      "__hash": "dd7c94641891f71ebb6a997ab07f735f91927449"
    },
    {
      "version": "0.1.15",
      "__hash": "99d1aaccc67cbf3ae85daa84e479c67408d4acdc"
    },
    {
      "version": "0.1.17",
      "__hash": "ce35b513bb8b996a6dbdb3a792e240157448c9d9"
    },
    {
      "version": "0.9.3",
      "build": "rm package.json && npm install --prefix . vue-resource@0.9.3",
      "description": "A web request service for Vue.js",
      "repos": "https://github.com/vuejs/vue-resource.git",
      "main": "vue-resource.common.js",
      "tag": "master",
      "reposType": "npm",
      "dependencies": [],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/vue-resource\\/(?:test|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/vue-resource\\/dist\\/(.*)$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ],
      "paths": {
        "dist": "."
      }
    }
  ]
}