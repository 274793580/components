{
  "repos": "https://github.com/vuejs/vue-resource",
  "description": "Resource component for Vue.js",
  "name": "vue-resource",
  "main": "vue-resource.js",
  "dependencies": [
    "vue@>=0.12.0"
  ],
  "mapping": [
    {
      "reg": "^\\/dist\\/(vue-resource\\.js)",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.1.13"
    },
    {
      "version": "0.1.15"
    },
    {
      "version": "0.1.17"
    }
  ]
}