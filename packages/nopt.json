{
  "build": "rm package.json && npm install --prefix . nopt@3.0.1",
  "name": "nopt",
  "description": "Option parsing for Node, supporting types, shorthands, etc. Used by npm.",
  "repos": "https://github.com/isaacs/nopt",
  "main": "lib/nopt.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "abbrev@1"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/nopt\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/nopt\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.0.1"
    },
    {
      "version": "3.0.2",
      "build": "rm package.json && npm install --prefix . nopt@3.0.2"
    },
    {
      "version": "3.0.3",
      "build": "rm package.json && npm install --prefix . nopt@3.0.3",
      "repos": "https://github.com/isaacs/nopt.git"
    },
    {
      "version": "3.0.4",
      "build": "rm package.json && npm install --prefix . nopt@3.0.4",
      "repos": "https://github.com/isaacs/nopt.git"
    },
    {
      "version": "3.0.5",
      "build": "rm package.json && npm install --prefix . nopt@3.0.5",
      "repos": "https://github.com/isaacs/nopt.git"
    },
    {
      "version": "3.0.6",
      "build": "rm package.json && npm install --prefix . nopt@3.0.6",
      "repos": "https://github.com/npm/nopt.git"
    }
  ]
}