{
  "name": "fit-checkbox",
  "description": "多选框",
  "repos": "https://github.com/2betop/placeholder.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "classnames@^2.2.1",
    "react@^0.14.3",
    "fit-style@^1.0.25"
  ],
  "build": "rm package.json && npm install --prefix . fit-checkbox@1.0.21",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-checkbox\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.21",
      "__hash": "b544cf79126706919d745e6e497c041154ee4017"
    },
    {
      "version": "1.0.22",
      "build": "rm package.json && npm install --prefix . fit-checkbox@1.0.22"
    },
    {
      "version": "1.0.23",
      "build": "rm package.json && npm install --prefix . fit-checkbox@1.0.23"
    }
  ]
}