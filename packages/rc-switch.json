{
  "name": "rc-switch",
  "description": "switch ui component for react",
  "repos": "https://github.com/react-component/switch.git",
  "main": "lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "rc-util@3.x"
  ],
  "build": "rm package.json && npm install --prefix . rc-switch@1.2.1",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-switch\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-switch\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.2.1"
    },
    {
      "version": "1.3.0",
      "build": "rm package.json && npm install --prefix . rc-switch@1.3.0"
    },
    {
      "version": "1.3.1",
      "build": "rm package.json && npm install --prefix . rc-switch@1.3.1"
    }
  ]
}