{
  "repos": "https://github.com/angular/bower-angular",
  "description": "HTML enhanced for web apps http://angularjs.org",
  "name": "angular",
  "main": "angular.js",
  "mapping": [
    {
      "reg": "^\\/angular\\.js$",
      "release": "$&"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "shim": {
    "angular.js": {
      "exports": "window.angular"
    }
  },
  "tags": [
    {
      "version": "v1.4.0"
    },
    {
      "version": "v1.3.14"
    }
  ]
}