{
  "build": "rm package.json && npm install --prefix . propagate@0.3.0",
  "name": "propagate",
  "description": "Propagate events",
  "repos": "https://github.com/pgte/propagate.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/propagate\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/propagate\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.3.0",
      "__hash": "edebe73abca78c6e8febb977f0e0091d2f193ca3"
    },
    {
      "version": "0.3.1",
      "build": "rm package.json && npm install --prefix . propagate@0.3.1"
    }
  ]
}