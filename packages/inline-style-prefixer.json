{
  "build": "rm package.json && npm install --prefix . inline-style-prefixer@0.6.1",
  "name": "inline-style-prefixer",
  "description": "Autoprefixer for inline styles using userAgent and caniuse data",
  "repos": "https://github.com/rofrischmann/inline-style-prefixer.git",
  "main": "prefixer",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "bowser@^1.0.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/inline-style-prefixer\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/inline-style-prefixer\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "paths": {
    "dist": "."
  },
  "tags": [
    {
      "version": "0.6.1"
    },
    {
      "version": "0.6.2",
      "build": "rm package.json && npm install --prefix . inline-style-prefixer@0.6.2"
    },
    {
      "version": "0.6.3",
      "build": "rm package.json && npm install --prefix . inline-style-prefixer@0.6.3"
    }
  ]
}