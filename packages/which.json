{
  "build": "rm package.json && npm install --prefix . which@1.1.1",
  "name": "which",
  "description": "Like which(1) unix command. Find the first instance of an executable in the PATH.",
  "repos": "https://github.com/isaacs/node-which.git",
  "main": "which.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "is-absolute@^0.1.7"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/which\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/which\\/(which\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.1.1"
    },
    {
      "version": "1.1.2",
      "build": "rm package.json && npm install --prefix . which@1.1.2"
    },
    {
      "version": "1.2.0",
      "build": "rm package.json && npm install --prefix . which@1.2.0"
    },
    {
      "version": "1.2.1",
      "build": "rm package.json && npm install --prefix . which@1.2.1"
    },
    {
      "version": "1.2.4",
      "build": "rm package.json && npm install --prefix . which@1.2.4"
    }
  ]
}