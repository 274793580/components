{
  "build": "rm package.json && npm install --prefix . define-properties@1.1.1",
  "name": "define-properties",
  "description": "Define multiple non-enumerable properties at once. Uses `Object.defineProperty` when available; falls back to standard assignment in older engines.",
  "repos": "https://github.com/ljharb/define-properties.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "foreach@^2.0.5",
    "object-keys@^1.0.7"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/define-properties\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/define-properties\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.1.1",
      "__hash": "f98c043db3e9e79cb13d9b15c7c9421ecc5c189a"
    },
    {
      "version": "1.1.2",
      "build": "rm package.json && npm install --prefix . define-properties@1.1.2"
    }
  ]
}