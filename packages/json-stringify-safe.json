{
  "build": "rm package.json && npm install --prefix . json-stringify-safe@5.0.1",
  "name": "json-stringify-safe",
  "description": "Like JSON.stringify, but doesn't blow up on circular refs.",
  "repos": "https://github.com/isaacs/json-stringify-safe.git",
  "main": "stringify.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/json-stringify-safe\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/json-stringify-safe\\/(stringify\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "5.0.0",
      "build": "rm package.json && npm install --prefix . json-stringify-safe@5.0.0",
      "description": "Like JSON.stringify, but doesn't blow up on circular refs",
      "repos": "https://github.com/isaacs/json-stringify-safe"
    },
    {
      "version": "5.0.1",
      "description": "Like JSON.stringify, but doesn't blow up on circular refs",
      "repos": "https://github.com/isaacs/json-stringify-safe"
    }
  ]
}