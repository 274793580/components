{
  "build": "rm package.json && npm install --prefix . recompose@0.15.0",
  "name": "recompose",
  "description": "A React utility belt for function components and higher-order components",
  "repos": "https://github.com/acdlite/recompose.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "hoist-non-react-statics@^1.0.0",
    "lodash@^4.0.0",
    "react@^0.14.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(mapProps\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(createHelper\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(wrapDisplayName\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(getDisplayName\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(createElement\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(isReferentiallyTransparentFunctionComponent\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(isClassComponent\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(mapPropsOnChange\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(shallowEqual\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(withProps\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(withAttachedProps\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(defaultProps\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(renameProp\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(renameProps\\.js)$",
      "release": "$1"
