    {
      "reg": "^\\/node_modules\\/recompose\\/(flattenProp\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(withState\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(withReducer\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(branch\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(renderComponent\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(renderNothing\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(shouldUpdate\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(pure\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(onlyUpdateForKeys\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(onlyUpdateForPropTypes\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(withContext\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(getContext\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(doOnReceiveProps\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(lifecycle\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(toClass\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(setStatic\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(setPropTypes\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(setDisplayName\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(compose\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(createSink\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(componentFromProp\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(nest\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/recompose\\/(hoistStatics\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.15.0"
    }
  ]
}