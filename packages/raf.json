{
  "build": "rm package.json && npm install --prefix . raf@3.1.0",
  "name": "raf",
  "description": "requestAnimationFrame polyfill for node and the browser",
  "repos": "https://github.com/chrisdickinson/raf.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "performance-now@~0.2.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/raf\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/raf\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.1.0"
    },
    {
      "version": "3.2.0",
      "build": "rm package.json && npm install --prefix . raf@3.2.0"
    }
  ]
}