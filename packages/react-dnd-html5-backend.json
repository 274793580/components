    },
    {
      "reg": "^\\/node_modules\\/react-dnd-html5-backend\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
