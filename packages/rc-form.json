{
  "build": "rm package.json && npm install --prefix . rc-form@0.12.1",
  "name": "rc-form",
  "description": "React High Order Form Component",
  "repos": "https://github.com/react-component/form.git",
  "main": "./lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "async-validator@^1.5.0",
    "hoist-non-react-statics@^1.0.3"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-form\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-form\\/(assets\\/\\*\\.css)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-form\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.12.1",
      "__hash": "2405b82fa4efebb963d6440c1641cab692fae3b9"
    }
  ]
}