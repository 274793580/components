{
  "build": "rm package.json && npm install --prefix . isemail@2.0.0",
  "name": "isemail",
  "description": "Validate an email address according to RFCs 5321, 5322, and others",
  "repos": "https://github.com/hapijs/isemail.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/isemail\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/isemail\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.0.0"
    },
    {
      "version": "2.1.0",
      "build": "rm package.json && npm install --prefix . isemail@2.1.0"
    }
  ]
}