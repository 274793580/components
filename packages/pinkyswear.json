{
  "build": "rm package.json && npm install --prefix . pinkyswear@2.2.2",
  "name": "pinkyswear",
  "description": "Minimalistic Promises/A+ implementation, <500 bytes",
  "repos": "https://github.com/timjansen/PinkySwear.js.git",
  "main": "pinkyswear.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/pinkyswear\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/pinkyswear\\/(pinkyswear\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "2.2.2"
    }
  ]
}