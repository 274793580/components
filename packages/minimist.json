{
  "build": "rm package.json && npm install --prefix . minimist@0.0.7",
  "name": "minimist",
  "description": "parse argument options",
  "repos": "https://github.com/substack/minimist.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/minimist\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/minimist\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.0.7"
    },
    {
      "version": "0.0.8",
      "build": "rm package.json && npm install --prefix . minimist@0.0.8"
    },
    {
      "version": "0.0.9",
      "build": "rm package.json && npm install --prefix . minimist@0.0.9"
    },
    {
      "version": "0.0.10",
      "build": "rm package.json && npm install --prefix . minimist@0.0.10"
    }
  ]
}