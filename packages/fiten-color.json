{
  "build": "rm package.json && npm install --prefix . fiten-color@1.0.2",
  "name": "fiten-color",
  "description": "样式",
  "repos": "https://github.com/2betop/placeholder.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fiten-color\\/(?:test|build|dist\\/cdn)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fiten-color\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.2"
    },
    {
      "version": "1.0.3",
      "build": "rm package.json && npm install --prefix . fiten-color@1.0.3"
    },
    {
      "version": "1.0.4",
      "build": "rm package.json && npm install --prefix . fiten-color@1.0.4"
    }
  ]
}