{
  "build": "rm package.json && npm install --prefix . mkdirp@0.5.0",
  "name": "mkdirp",
  "description": "Recursively mkdir, like `mkdir -p`",
  "repos": "https://github.com/substack/node-mkdirp.git",
  "main": "index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/mkdirp\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/mkdirp\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.5.0"
    },
    {
      "version": "0.5.1",
      "build": "rm package.json && npm install --prefix . mkdirp@0.5.1"
    }
  ]
}