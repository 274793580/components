{
  "repos": "https://github.com/aFarkas/html5shiv.git",
  "name": "html5shiv",
  "main": "html5shiv.js",
  "mapping": [
    {
      "reg": "\\.min\\.(js|css)$",
      "release": false
    },
    {
      "reg": "^\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.7.2"
    }
  ]
}