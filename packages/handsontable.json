{
  "build": "rm package.json && npm install --prefix . handsontable@0.23.0",
  "name": "handsontable",
  "description": "Spreadsheet-like data grid editor that provides copy/paste functionality compatible with Excel/Google Docs",
  "repos": "https://github.com/handsontable/handsontable.git",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "moment@^2.9.0",
    "pikaday@^1.3.2",
    "zeroclipboard@^2.2.0"
  ],
  "browser": null,
  "main": "handsontable.js",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/handsontable\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/handsontable\\/dist\\/(?:moment|pikaday|zeroclipboard)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/handsontable\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "paths": {
    "dist": "."
  },
  "tags": [
    {
      "version": "0.23.0",
      "browser": null,
      "__hash": "df67f3ecf6dcb7569600215f67eeda6fbd7300d0"
    }
  ]
}