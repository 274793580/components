{
  "build": "rm package.json && npm install --prefix . react-document-title@1.0.4",
  "name": "react-document-title",
  "description": "A declarative, nested, stateful document.title for React",
  "repos": "https://github.com/gaearon/react-document-title.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "react-side-effect@^0.3.2"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-document-title\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-document-title\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.4",
      "__hash": "aca669b87ae21198f0f0e5d8efe8a728614e433d"
    },
    {
      "version": "2.0.0",
      "build": "rm package.json && npm install --prefix . react-document-title@2.0.0"
    },
    {
      "version": "2.0.1",
      "build": "rm package.json && npm install --prefix . react-document-title@2.0.1",
      "dependencies": [
        "react-side-effect@^1.0.0"
      ]
    }
  ]
}