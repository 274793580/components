{
  "name": "semver",
  "description": "The semantic version parser used by npm.",
  "repos": "https://github.com/npm/node-semver.git",
  "main": "semver.js",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . semver@5.0.2",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/semver\\/(.*\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "5.0.2",
      "dependencies": [],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/semver\\/(?:test|build|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/semver\\/(semver\\.js)$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    },
    {
      "version": "5.0.3",
