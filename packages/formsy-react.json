{
  "repos": "https://github.com/christianalfoni/formsy-react.git",
  "name": "formsy-react",
  "main": "lib/index.js",
  "build": "rm package.json && npm install --prefix . formsy-react@0.17.0",
  "dependencies": [
    "form-data-to-object@^0.2.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/formsy-react\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "umd2commonjs": false,
  "tags": [
    {
      "umd2commonjs": false,
      "version": "v0.17.0",
      "__hash": "e0269d24327f1f25913b58ced2f80917285d4b51"
    },
    {
      "version": "0.17.0",
      "description": "A form input builder and validator for React JS",
      "main": "lib/main.js",
      "tag": "master",
      "reposType": "npm",
      "dependencies": [
        "form-data-to-object@^0.1.0",
        "react@^0.14.0"
      ],
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/formsy-react\\/(?:test|build|dist\\/cdn|lib\\/node)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/formsy-react\\/lib\\/(.*)$",
          "release": "lib/$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ],
      "umd2commonjs": null
    }
  ]
}