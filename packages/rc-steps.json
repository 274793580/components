{
  "name": "rc-steps",
  "description": "steps ui component for react",
  "repos": "https://github.com/react-component/steps.git",
  "main": "./lib/index",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . rc-steps@1.3.3",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-steps\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-steps\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.3.3"
    },
    {
      "version": "1.4.0",
      "build": "rm package.json && npm install --prefix . rc-steps@1.4.0",
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/rc-steps\\/lib\\/(.*)$",
          "release": "lib/$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "release": false
        }
      ]
    },
    {
      "version": "1.4.1",
      "build": "rm package.json && npm install --prefix . rc-steps@1.4.1"
    }
  ]
}