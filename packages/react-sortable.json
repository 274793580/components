{
  "build": "rm package.json && npm install --prefix . react-sortable@0.0.1",
  "name": "react-sortable",
  "description": "A sortable list component for React",
  "repos": "https://github.com/danielstocks/react-sortable.git",
  "main": "src/Sortable.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-sortable\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-sortable\\/(src\\/Sortable\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.0.1"
    },
    {
      "version": "0.0.2",
      "build": "rm package.json && npm install --prefix . react-sortable@0.0.2"
    }
  ]
}