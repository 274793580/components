{
  "repos": "https://github.com/moment/moment.git",
  "description": "Parse, validate, manipulate, and display dates in javascript. http://momentjs.com",
  "name": "moment",
  "main": "moment.js",
  "mapping": [
    {
      "reg": "^\\/moment\\.js$",
      "release": "$&"
    },
    {
      "reg": "^\\/locale\\/.*$",
      "release": "$&"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "umd2commonjs": false,
  "tags": [
    {
      "version": "2.10.6",
      "build": "rm package.json && npm install --prefix . moment@2.10.6",
      "description": "Parse, validate, manipulate, and display dates",
      "main": "./moment.js",
      "tag": "master",
      "reposType": "npm",
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/moment\\/(?:test|build|dist\\/cdn)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/moment\\/(.*\\.(?:js|css))$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    },
    {
      "version": "2.11.0",
      "build": "rm package.json && npm install --prefix . moment@2.11.0",
      "description": "Parse, validate, manipulate, and display dates",
      "main": "./moment.js",
      "tag": "master",
      "reposType": "npm",
      "mapping": [
        {
          "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/moment\\/(?:test|build|dist\\/cdn)\\/",
          "release": false
        },
        {
          "reg": "^\\/node_modules\\/moment\\/(.*\\.(?:js|css))$",
          "release": "$1"
        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ]
    }
  ]
}