{
  "build": "rm package.json && npm install --prefix . ctype@0.5.3",
  "name": "ctype",
  "description": "read and write binary structures and data types",
  "repos": "https://github.com/rmustacc/node-ctype.git",
  "main": "ctype.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/ctype\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/ctype\\/(ctype\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/ctype\\/(ctf\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/ctype\\/(ctio\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.5.3",
      "__hash": "ea1a2900c28ba0f6439ad21cf0ad25112119eaa3"
    }
  ]
}