        },
        {
          "reg": "^\\/README\\.md$",
          "release": "$&"
        },
        {
          "reg": "^\\/LICENSE\\.md$",
          "release": "$&"
        },
        {
          "reg": "\\.*",
          "release": false
        }
      ],
      "paths": {
        "./asap.js": "./browser-asap.js",
        "./raw.js": "./browser-raw.js",
        "./test/domain.js": "./test/browser-domain.js"
      },
      "shim": {
        "**/*.js": {
          "replace": [
            {
              "from": "/\\brequire\\s*\\(\\s*('|\")\\.\\/asap(?:\\.js)?\\b/ig",
              "to": "require($1./browser-asap.js"
            },
            {
              "from": "/\\brequire\\s*\\(\\s*('|\")\\.\\/raw(?:\\.js)?\\b/ig",
