{
  "build": "rm package.json && npm install --prefix . react-validation-mixin@5.3.2",
  "name": "react-validation-mixin",
  "description": "Simple Validation Mixin for React.",
  "repos": "https://github.com/jurassix/react-validation-mixin.git",
  "main": "lib/index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "invariant@^2.1.0",
    "lodash.flatten@^3.0.2",
    "lodash.get@^3.7.0",
    "lodash.isempty@^3.0.4",
    "lodash.result@^3.1.2",
    "lodash.values@^3.0.0",
    "react-display-name@0.0.1"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-validation-mixin\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/react-validation-mixin\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "5.3.2"
    },
    {
      "version": "5.3.3",
      "build": "rm package.json && npm install --prefix . react-validation-mixin@5.3.3"
    },
    {
      "version": "5.3.4",
      "build": "rm package.json && npm install --prefix . react-validation-mixin@5.3.4",
      "main": "./lib/index.js"
    }
  ]
}