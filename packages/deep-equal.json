{
  "name": "deep-equal",
  "description": "node's assert.deepEqual algorithm",
  "repos": "https://github.com/substack/node-deep-equal.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . deep-equal@1.0.1",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/deep-equal\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/deep-equal\\/(.*\\.(?:js|css))$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.1"
    }
  ]
}