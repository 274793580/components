  "main": "lib/index",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "rc-animate@2.x",
    "rc-util@3.x"
  ],
  "build": "rm package.json && npm install --prefix . rc-notification@1.1.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
