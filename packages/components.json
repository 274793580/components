{
  "name": "components",
  "description": "Upstage component library.",
  "repos": "https://github.com/assemble/components.git",
  "main": "Gruntfile.js",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . components@0.0.0",
  "mapping": [
    {
      "reg": "/\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js/i",
      "release": false
    },
    {
      "reg": "/^\\/node_modules\\/components\\/(.*\\.(?:js|css))$/i",
      "release": "$1"
    },
    {
      "reg": "/^\\/README\\.md$/i",
      "release": "$&"
    },
    {
      "reg": "/^\\/LICENSE\\.md$/i",
      "release": "$&"
    },
    {
      "reg": "*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.0.0"
    },
    {
      "version": "0.1.0",
      "build": "rm package.json && npm install --prefix . components@0.1.0",
      "mapping": [
        {
          "reg": "/\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js/i",
          "release": false
        },
        {
          "reg": "/^\\/node_modules\\/components\\/(.*\\.(?:js|css))$/i",
          "release": "$1"
        },
        {
          "reg": "/^\\/node_modules\\/components\\/assets\\/(.*)$/i",
          "release": "assets/$1"
        },
        {
          "reg": "/^\\/README\\.md$/i",
          "release": "$&"
        },
        {
          "reg": "/^\\/LICENSE\\.md$/i",
          "release": "$&"
        },
        {
          "reg": "*",
          "release": false
        }
      ]
    }
  ]
}