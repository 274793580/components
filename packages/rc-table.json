{
  "name": "rc-table",
  "description": "table ui component for react",
  "repos": "https://github.com/react-component/table.git",
  "main": "./lib/index",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . rc-table@3.6.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/rc-table\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/node_modules\\/rc-table\\/assets\\/(.*)$",
      "release": "assets/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.6.0"
    },
    {
      "version": "3.6.1",
      "build": "rm package.json && npm install --prefix . rc-table@3.6.1"
    },
    {
      "version": "3.6.2",
      "build": "rm package.json && npm install --prefix . rc-table@3.6.2"
    }
  ]
}