{
  "build": "rm package.json && npm install --prefix . json3@3.3.2",
  "name": "json3",
  "description": "A modern JSON implementation compatible with nearly all JavaScript platforms.",
  "repos": "https://github.com/bestiejs/json3.git",
  "main": "lib/json3",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/json3\\/(?:test|build|dist\\/cdn)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/json3\\/(lib\\/json3\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.3.2"
    }
  ]
}