{
  "build": "rm package.json && npm install --prefix . lodash.toplainobject@3.0.0",
  "name": "lodash.toplainobject",
  "description": "The modern build of lodashâ€™s `_.toPlainObject` as a module.",
  "repos": "https://github.com/lodash/lodash",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "lodash._basecopy@^3.0.0",
    "lodash.keysin@^3.0.0"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/lodash\\.toplainobject\\/(?:test|build|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/lodash\\.toplainobject\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "main": "index.js",
  "tags": [
    {
      "version": "3.0.0"
    }
  ]
}