{
  "repos": "https://github.com/codemirror/codemirror",
  "description": "In-browser code editor http://codemirror.net/",
  "name": "codemirror",
  "main": "codemirror.js",
  "mapping": [
    {
      "reg": "^\\/lib\\/(.*\\.(css|js))$",
      "release": "$&"
    },
    {
      "reg": "^\\/mode\\/(.*\\.(css|js))$",
      "release": "$&"
    },
    {
      "reg": "^\\/addon\\/(.*\\.(css|js))$",
      "release": "$&"
    },
    {
      "reg": "^\\/theme\\/(.*)$",
      "release": "$&"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^.*$",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "5.6.0",
      "__hash": "b062544db26dcda73e39de5f180facee4dbb38a3"
    },
    {
      "version": "5.9.0",
      "__hash": "be634e576f3460f1d1353bf1ec0ade19e72555bd"
    }
  ]
}