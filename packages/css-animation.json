{
  "name": "css-animation",
  "description": "css-animation ui component for react",
  "repos": "https://github.com/yiminghe/css-animation.git",
  "main": "./lib/index",
  "tag": "master",
  "reposType": "npm",
  "build": "rm package.json && npm install --prefix . css-animation@1.1.0",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/css-animation\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.1.0",
      "__hash": "edbd8e021a8551fc60c85f362120bc631f45ee29"
    },
    {
      "version": "1.1.1",
      "build": "rm package.json && npm install --prefix . css-animation@1.1.1"
    }
  ]
}