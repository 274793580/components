{
  "name": "fit-layout-global",
  "description": "全局布局",
  "repos": "https://github.com/2betop/placeholder.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "jquery@^2.1.4",
    "redux@^3.0.5",
    "fit-style@^1.0.28"
  ],
  "build": "rm package.json && npm install --prefix . fit-layout-global@0.1.26",
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fit-layout-global\\/dist\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.1.26"
    },
    {
      "version": "0.1.28",
      "build": "rm package.json && npm install --prefix . fit-layout-global@0.1.28"
    },
    {
      "version": "0.1.29",
      "build": "rm package.json && npm install --prefix . fit-layout-global@0.1.29"
    }
  ]
}