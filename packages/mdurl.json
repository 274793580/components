{
  "build": "rm package.json && npm install --prefix . mdurl@1.0.0",
  "name": "mdurl",
  "description": "URL utilities for markdown-it",
  "repos": "https://github.com/markdown-it/mdurl",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/mdurl\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/mdurl\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/mdurl\\/(encode\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/mdurl\\/(decode\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/mdurl\\/(format\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/mdurl\\/(parse\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "main": "index.js",
  "tags": [
    {
      "version": "1.0.0"
    },
    {
      "version": "1.0.1",
      "build": "rm package.json && npm install --prefix . mdurl@1.0.1"
    }
  ]
}