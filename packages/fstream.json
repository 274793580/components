{
  "build": "rm package.json && npm install --prefix . fstream@1.0.2",
  "name": "fstream",
  "description": "Advanced file system stream things",
  "repos": "https://github.com/isaacs/fstream.git",
  "main": "fstream.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "graceful-fs@3",
    "inherits@~2.0.0",
    "mkdirp@>=0.5 0",
    "rimraf@2"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fstream\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/fstream\\/(fstream\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/fstream\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.2",
      "__hash": "998e37b7f4da2cea1ef14f37ad32ee95a0b71e1f"
    },
