    },
    {
      "version": "1.0.7",
      "build": "rm package.json && npm install --prefix . fstream@1.0.7"
    },
    {
      "version": "1.0.8",
      "build": "rm package.json && npm install --prefix . fstream@1.0.8"
    }
  ]
