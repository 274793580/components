{
  "build": "rm package.json && npm install --prefix . keymirror@0.1.0",
  "name": "keymirror",
  "description": "A simple utility for creating an object with values equal to its keys. Identical to react/lib/keyMirror",
  "repos": "https://github.com/2betop/placeholder.git",
  "main": "index.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/keymirror\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/keymirror\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "0.1.0"
    },
    {
      "version": "0.1.1",
      "build": "rm package.json && npm install --prefix . keymirror@0.1.1"
    }
  ]
}