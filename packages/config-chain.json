{
  "build": "rm package.json && npm install --prefix . config-chain@1.1.8",
  "name": "config-chain",
  "description": "HANDLE CONFIGURATION ONCE AND FOR ALL",
  "repos": "https://github.com/dominictarr/config-chain.git",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [
    "proto-list@~1.2.1",
    "ini@1"
  ],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/config-chain\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/config-chain\\/(index\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "main": "index.js",
  "tags": [
    {
      "version": "1.1.8"
    },
    {
      "version": "1.1.9",
      "build": "rm package.json && npm install --prefix . config-chain@1.1.9"
    },
    {
      "version": "1.1.10",
      "build": "rm package.json && npm install --prefix . config-chain@1.1.10"
    }
  ]
}