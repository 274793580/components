{
  "build": "rm package.json && npm install --prefix . hawk@3.1.0",
  "name": "hawk",
  "description": "HTTP Hawk Authentication Scheme",
  "repos": "https://github.com/hueniverse/hawk.git",
  "main": "./lib/browser.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/hawk\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/hawk\\/(lib\\/browser\\.js)$",
      "release": "$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "3.1.0"
    },
    {
      "version": "3.1.1",
      "build": "rm package.json && npm install --prefix . hawk@3.1.1"
    },
    {
      "version": "3.1.2",
      "build": "rm package.json && npm install --prefix . hawk@3.1.2"
    },
    {
      "version": "3.1.3",
      "build": "rm package.json && npm install --prefix . hawk@3.1.3"
    }
  ]
}