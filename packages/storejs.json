{
  "build": "rm package.json && npm install --prefix . storejs@1.0.6",
  "name": "storejs",
  "description": "Local storage localstorage package provides a simple API",
  "repos": "https://github.com/jaywcjlove/store.js.git",
  "main": "lib/store.js",
  "tag": "master",
  "reposType": "npm",
  "dependencies": [],
  "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/storejs\\/(?:test|dist\\/cdn|lib\\/node)\\/",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/storejs\\/lib\\/(.*)$",
      "release": "lib/$1"
    },
    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }
  ],
  "tags": [
    {
      "version": "1.0.6"
    },
    {
      "version": "1.0.7",
      "build": "rm package.json && npm install --prefix . storejs@1.0.7"
    },
    {
      "version": "1.0.8",
      "build": "rm package.json && npm install --prefix . storejs@1.0.8"
    }
  ]
}