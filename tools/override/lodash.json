{
  "mapping": [
      {
        "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
        "release": false
      },
      {
        "reg": "^\\/node_modules\\/lodash\\/(?:test|dist\\/cdn|lib\\/node)\\/",
        "release": false
      },
      {
        "reg": "^\\/node_modules\\/lodash\\/(.*\\.js)$",
        "release": "$1"
      },
      {
        "reg": "^\\/README\\.md$",
        "release": "$&"
      },
      {
        "reg": "^\\/LICENSE\\.md$",
        "release": "$&"
      },
      {
        "reg": "\\.*",
        "release": false
      }
    ]
}
