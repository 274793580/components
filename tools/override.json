{
  "rc-collapse": {
    "repos": "https://github.com/react-component/collapse.git"
  },
  "bootstrap": {
    "main": "bootstrap.js",
    "mapping": [
    {
      "reg": "\\bmin\\b|__tests__|gulpfile\\.js|webpack\\.config\\.js|gruntfile\\.js|test\\.js",
      "release": false
    },
    {
      "reg": "^\\/node_modules\\/bootstrap\\/dist\\/js\\/umd\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/bootstrap\\/dist\\/js\\/(.*)$",
      "release": "$1"
    },
    {
      "reg": "^\\/node_modules\\/bootstrap\\/dist\\/(.*)$",
      "release": "$1"
    },

    {
      "reg": "^\\/README\\.md$",
      "release": "$&"
    },
    {
      "reg": "^\\/LICENSE\\.md$",
      "release": "$&"
    },
    {
      "reg": "\\.*",
      "release": false
    }],
    "dependencies": [
      "jquery@>=1.9.1"
    ],
    "shim": {
      "bootstrap.js": {
        "content": "require('./npm.js');"
      },
      "alert.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "button.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "carousel.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "collapse.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "dropdown.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "modal.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "popover.js": {
        "deps": [
          "jquery",
          "./tooltip.js"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "scrollspy.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "tab.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "tooltip.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      },
      "util.js": {
        "deps": [
          "jquery"
        ],
        "vars": [
          "jQuery"
        ]
      }
    }
  }
}
